generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model gastos_fijos {
  id          Int      @id @default(autoincrement())
  nombre      String   @db.VarChar(100)
  monto       Float?
  pagado      Boolean?
  fecha       String?  @db.VarChar(20)
  comentarios String?  @db.Text
}
